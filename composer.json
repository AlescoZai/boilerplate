{
    "name": "sebastienheyd/boilerplate",
    "description": "Boilerplate with AdminLTE, user management, roles, permissions for Laravel >= 5.4",
    "keywords": ["laravel", "adminlte", "laratrust", "laravelcollective", "active", "admin"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.7.*",
        "laravelcollective/html": "^5.5",
        "santigarcor/laratrust": "^3.2",
        "hieu-le/active": "^3.4",
        "yajra/laravel-datatables-oracle": "^8.0",
        "lavary/laravel-menu": "^1.6",
        "jenssegers/date": "^3.2",
        "intervention/image": "^2.3",
        "arcanedev/log-viewer": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "^3.7",
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Sebastienheyd\\Boilerplate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebastienheyd\\Boilerplate\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "SÃ©bastien HEYD",
            "email": "contact@sheyd.fr"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Sebastienheyd\\Boilerplate\\BoilerplateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
