<?php

namespace App\Menu;

use Sebastienheyd\Boilerplate\Menu\Builder;

class {{NAME}}
{
    public function make(Builder $menu)
    {
        $menu->add('{{NAME}}', ['route' => 'boilerplate.dashboard', 'permission' => 'backend', 'icon' => 'square'])
             ->id('{{NAME}}')
             ->activeIfRoute('boilerplate.dashboard')
             ->order({{ORDER}});
    }
}
