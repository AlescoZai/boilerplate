{
    "name": "sebastienheyd/boilerplate",
    "description": "Laravel ≥5.4 Boilerplate based on AdminLTE with user management, roles, permissions, logs viewer, ...",
    "keywords": ["laravel", "boilerplate", "adminlte", "laratrust", "laravelcollective", "active", "admin"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "laravel/framework": "6.*",
        "laravelcollective/html": "^6.0",
        "santigarcor/laratrust": "^5.2",
        "hieu-le/active": "^4.0",
        "yajra/laravel-datatables-oracle": "^9.0",
        "lavary/laravel-menu": "^1.7",
        "intervention/image": "^2.5",
        "arcanedev/log-viewer": "^5.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "^4.0",
        "doctrine/dbal": "^2.9"
    },
    "suggest": {
        "sebastienheyd/boilerplate-media-manager": "Media manager",
        "sebastienheyd/boilerplate-email-editor": "Visual editor for e-mails"
    },
    "autoload": {
        "psr-4": {
            "Sebastienheyd\\Boilerplate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebastienheyd\\Boilerplate\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sébastien HEYD",
            "email": "contact@sheyd.fr"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Sebastienheyd\\Boilerplate\\BoilerplateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
